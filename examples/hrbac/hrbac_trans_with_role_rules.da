from hrbac_py import HRBAC_py

class HRBAC_trans_with_role_rules(HRBAC_py,process):
  
  def rules_trans_with_role_rules(): # with additional last rule
    path(x,y), if_(edge(x,y))
    path(x,y), if_(edge(x,z), path(z,y))
    path(x,x), if_(role(x))
  
  def trans(E):  # use infer only, pass in also ROLES
    return infer(rule=rules_trans_with_role_rules, bindings=[('edge',E),('role',ROLES)], queries=['path'])
