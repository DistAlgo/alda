queryrule(X,T,Y,A,M) :- att(X,title,T),att(X,year,Y),att(X,author,A),att(X,month,M).

measureTime(Q, T, WT) :-
        walltime(WT0),
	cputime(T0),
	call(Q),
	cputime(T1),
	walltime(WT1),
	T is T1 - T0,
	WT is WT1 - WT0.

